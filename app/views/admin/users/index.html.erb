<% content_for :heading do %>
   Listing Users
<%end%>


<table class='listing'>
  <tr>
    <th> Name </th>
    <th> Email </th>
    <th> </th>
  </tr>

<% @users.each do |user| %>
  <tr>
     <td> <%= user.name  %> </td>
     <td> <%= user.email %> </td>
     <td> <%= 'facebook user' if user.provider == 'facebook' %> </td>
  </tr>
<% end %>
</table>

