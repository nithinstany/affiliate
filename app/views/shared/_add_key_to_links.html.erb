<% if current_user %>
    <% content_for :extra_javascripts do %>
      <script type="text/javascript">
        $(function()
          {
             $("#listings > a").each(function ()
             {
                var href = $(this).attr('href');
                $(this).attr('href', href + "&u1=<%=current_user.key%>");
             });


          });
      </script>
    <%end%>
  <%end%>

