<% if current_user %>
    <% content_for :extra_javascripts do %>
      <script type="text/javascript">
        $(function()
          {
             $(".banner > a").each(function ()
             {
                var href = $(this).attr('href');
                $(this).attr('href', href + "&u1=<%=current_user.key%>");
             });
          });
      </script>
    <%end%>
 <%else%>
   <% content_for :extra_javascripts do %>
      <script type="text/javascript">
        $(function()
          {
             $(".rightContent .left > a").click(function(event) {
               window.location = "<%= new_user_session_path(:login => true) %>";
               return false;
             });
          });
      </script>
    <%end%>
 <%end%>

