<% listings.featured.in_groups_of(3) do |group| %>
  <% group.each do |listing| %>
    <% unless listing.blank? %>
      <div class='bannerBox left'>
        <div class="banner"><%= raw(listing.banner) %></div>
        <p><%= listing.notes %></p>
      </div>
    <%end%>
  <%end%>
  <div class='clear'> </div>
<%end%>

